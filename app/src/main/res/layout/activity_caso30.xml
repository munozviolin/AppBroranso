<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/background_light"
    android:id="@+id/C30"
    tools:context="com.tcu.munozviolin.appbroranso.Caso30">

    <TextView
        android:id="@+id/textView30"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:fontFamily="sans-serif"
        android:text="m"
        android:textColor="@android:color/black"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/buttonCaso30"
        android:layout_width="95dp"
        android:layout_height="37dp"
        android:layout_marginStart="52dp"
        android:layout_marginTop="64dp"
        android:background="@color/colorPrimary"
        android:fontFamily="sans-serif"
        android:text="fómcuo"
        android:textAllCaps="?android:attr/windowSwipeToDismiss"
        android:textColor="@android:color/holo_green_light"
        android:textColorLink="@android:color/black"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/buttonCaso30_1"
        android:layout_width="95dp"
        android:layout_height="37dp"
        android:layout_marginStart="52dp"
        android:layout_marginTop="95dp"
        android:background="@color/colorPrimary"
        android:fontFamily="sans-serif"
        android:text="dogóm"
        android:textAllCaps="?android:attr/windowSwipeToDismiss"
        android:textColor="@android:color/holo_green_light"
        android:textColorLink="@android:color/black"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageButton30_1" />

    <ImageView
        android:id="@+id/gifIzq30"
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/buttonCaso30"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.498" />

    <ImageView
        android:id="@+id/gifDer30"
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/buttonCaso30"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.498" />

    <ImageButton
        android:id="@+id/imageButton30_1"
        android:layout_width="81dp"
        android:layout_height="79dp"
        android:layout_marginStart="59dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/fomcuo"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/buttonCaso30"
        app:srcCompat="@android:color/transparent" />

    <ImageButton
        android:id="@+id/imageButton30_2"
        android:layout_width="83dp"
        android:layout_height="81dp"
        android:layout_marginStart="58dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/dogom"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/buttonCaso30_1"
        app:srcCompat="@android:color/transparent" />

</android.support.constraint.ConstraintLayout>