<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/background_light"
    android:id="@+id/C29"
    tools:context="com.tcu.munozviolin.appbroranso.Caso29">

    <TextView
        android:id="@+id/textView29"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:fontFamily="@font/margarine"
        android:text="zh"
        android:textColor="@android:color/black"
        android:textSize="29sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/buttonCaso29"
        android:layout_width="95dp"
        android:layout_height="37dp"
        android:layout_marginStart="52dp"
        android:layout_marginTop="64dp"
        android:background="@color/colorPrimary"
        android:fontFamily="@font/margarine"
        android:text="zhír"
        android:textAllCaps="?android:attr/windowSwipeToDismiss"
        android:textColor="@android:color/holo_green_light"
        android:textColorLink="@android:color/black"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/buttonCaso29_1"
        android:layout_width="95dp"
        android:layout_height="37dp"
        android:layout_marginStart="52dp"
        android:layout_marginTop="122dp"
        android:background="@color/colorPrimary"
        android:fontFamily="@font/margarine"
        android:text="quë́zhbon̈́"
        android:textAllCaps="?android:attr/windowSwipeToDismiss"
        android:textColor="@android:color/holo_green_light"
        android:textColorLink="@android:color/black"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageButton29_1" />

    <ImageView
        android:id="@+id/gifIzq29"
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/buttonCaso29"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.498" />

    <ImageView
        android:id="@+id/gifDer29"
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/buttonCaso29"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.498" />

    <ImageButton
        android:id="@+id/imageButton29_1"
        android:layout_width="78dp"
        android:layout_height="48dp"
        android:layout_marginStart="61dp"
        android:layout_marginTop="19dp"
        android:background="@drawable/zhir"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/buttonCaso29"
        app:srcCompat="@android:color/transparent" />

    <ImageButton
        android:id="@+id/imageButton29_2"
        android:layout_width="83dp"
        android:layout_height="59dp"
        android:layout_marginStart="57dp"
        android:layout_marginTop="19dp"
        android:background="@drawable/quezhbon"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/buttonCaso29_1"
        app:srcCompat="@android:color/transparent" />

</android.support.constraint.ConstraintLayout>